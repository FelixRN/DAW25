		System.out.println("Welcome");
		System.out.println("Ingrese la edad del alumno");
		edad = teclado.nextInt();
		
		if (edad >=4 && edad <=6) {
			System.out.println("El horario G.KINDER es Lunes y MiÃ©rcoles de 16 a 17hs");
		}else {
			if (edad >=7 && edad <=8) {
				System.out.println("El horario G.firts year es Martes y Juevesde 17:30 a 18hs");

			}
		}
	}
}
